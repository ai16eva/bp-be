{
  "name": "api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=prod nodemon server.js",
    "start:dev": "NODE_ENV=dev nodemon server.js",
    "deploy": "NODE_ENV=prod forever start server.js",
    "db:clear": "mysql -h 127.0.0.1 -u root -p boomplay_db < scripts/helpers/clear_db_data.sql",
    "swagger-autogen": "node swagger/swagger.js",
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:solana": "node src/utils/solana-test.js",
    "test:solana:quick": "node -e \"require('./src/utils/solana-test.js').quickHealthCheck().then(console.log)\"",
    "build:solana": "cd src/solana-sdk && npx tsc",
    "test:solana:build": "npm run build:solana && npm run test:solana",
    "e2e:solana:quick": "NODE_ENV=dev node -e \"require('./src/utils/solana-test.js').quickHealthCheck().then(console.log)\"",
    "e2e:full": "NODE_ENV=dev node scripts/e2e_full_flow.js",
    "e2e:enhanced": "NODE_ENV=dev node scripts/e2e_enhanced_flow.js",
    "e2e:validation": "NODE_ENV=dev node scripts/e2e_validation_tests.js",
    "e2e:suite": "NODE_ENV=dev node scripts/e2e_test_suite.js",
    "e2e:all": "npm run e2e:suite && npm run e2e:validation && npm run e2e:enhanced",
    "vote:three": "NODE_ENV=dev node scripts/tools/vote_three_times.js",
    "vote:decision": "NODE_ENV=dev node scripts/tools/vote_decision_three_times.js",
    "e2e:ui-flow": "NODE_ENV=dev node scripts/tests/test_ui_full_flow.js",
    "setup-webhook": "node scripts/setup-webhook.js",
    "update-webhook": "node scripts/update-webhook.js",
    "solana:install": "npm install --prefix src/solana-sdk",
    "solana:prepare": "npm run solana:install && npm run build:solana",
    "prestart": "npm run build:solana",
    "prestart:dev": "npm run build:solana",
    "predeploy": "npm run build:solana",
    "postinstall": "npm run solana:prepare"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.379.1",
    "@coral-xyz/anchor": "^0.30.1",
    "@metaplex-foundation/digital-asset-standard-api": "^2.0.0",
    "@metaplex-foundation/js": "^0.20.1",
    "@metaplex-foundation/mpl-core-das": "^0.1.0",
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@metaplex-foundation/umi-bundle-defaults": "^1.4.1",
    "@solana/spl-token": "^0.4.6",
    "@solana/web3.js": "^1.95.2",
    "@truffle/hdwallet-provider": "^2.1.15",
    "aws-sdk": "^2.1425.0",
    "axios": "^1.12.2",
    "bs58": "^5.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "form-data": "^4.0.5",
    "http-errors": "~1.6.3",
    "i": "^0.3.7",
    "jsonwebtoken": "^9.0.1",
    "mariadb": "^3.3.0",
    "moment": "^2.30.1",
    "morgan": "~1.9.1",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "mysql2": "^3.5.2",
    "node-cron": "^3.0.2",
    "nodemailer": "^6.9.4",
    "nodemon": "^3.0.1",
    "pug": "^3.0.2",
    "sequelize": "^6.32.1",
    "sqlite3": "^5.1.6",
    "swagger-autogen": "^2.23.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.0",
    "web3": "^4.16.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "sequelize-cli": "^6.6.1",
    "supertest": "^7.0.0"
  }
}
